extends layout

block content
    h1 Contact
    h3 active-users
    input#userA_ID(type="hidden", name="userA_ID", value= userA_ID)
    ul#list-active-users
    h3 contacts
    ul#list-contacts
        //handle contact
        if listActiveUsers
            each val in listActiveUsers
                li
                    a(href= "/room/" + val.oauthID)= val.displayName
block script
    //script(src="/room/minion/socket.io/socket.io.js")
    script(src="/socket.io/socket.io.js")
    //script(src="https://cdn.socket.io/socket.io-1.4.5.js")
    script(src="/javascripts/contact-active-users.js")